<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.motion.widget.MotionLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layoutDescription="@xml/activity_easter_eggs11_scene"
    app:showPaths="true"
    android:id="@+id/motionLayout">


    <ImageViews
        android:id="@+id/center"
        android:layout_width="160dp"
        android:layout_height="160dp"
        android:src="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/controlPoint"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="@id/center"
        app:layout_constraintCircle="@id/center"
        app:layout_constraintCircleAngle="120"
        app:layout_constraintCircleRadius="50dp"
        app:layout_constraintStart_toStartOf="@id/center"
        app:tint="@android:color/white" />


    <ImageView
        android:id="@+id/point0"
        android:layout_width="6dp"
        android:layout_height="6dp"
        android:src="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="@id/center"
        app:layout_constraintCircle="@id/center"
        app:layout_constraintCircleAngle="120"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintStart_toStartOf="@id/center"
        app:tint="@android:color/black" />

    <ImageView
        android:id="@+id/point1"
        android:layout_width="6dp"
        android:layout_height="6dp"
        android:src="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="@id/center"
        app:layout_constraintCircle="@id/center"
        app:layout_constraintCircleAngle="150"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintStart_toStartOf="@id/center"
        app:tint="@android:color/black" />

    <ImageView
        android:id="@+id/point2"
        android:layout_width="6dp"
        android:layout_height="6dp"
        android:src="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="@id/center"
        app:layout_constraintCircle="@id/center"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintStart_toStartOf="@id/center"
        app:tint="@android:color/black" />

    <ImageView
        android:id="@+id/point3"
        android:layout_width="6dp"
        android:layout_height="6dp"
        android:src="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="@id/center"
        app:layout_constraintCircle="@id/center"
        app:layout_constraintCircleAngle="210"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintStart_toStartOf="@id/center"
        app:tint="@android:color/black" />

    <ImageView
        android:id="@+id/point4"
        android:layout_width="6dp"
        android:layout_height="6dp"
        android:src="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="@id/center"
        app:layout_constraintCircle="@id/center"
        app:layout_constraintCircleAngle="240"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintStart_toStartOf="@id/center"
        app:tint="@android:color/black" />

    <ImageView
        android:id="@+id/point5"
        android:layout_width="6dp"
        android:layout_height="6dp"
        android:src="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="@id/center"
        app:layout_constraintCircle="@id/center"
        app:layout_constraintCircleAngle="270"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintStart_toStartOf="@id/center"
        app:tint="@android:color/black" />

    <ImageView
        android:id="@+id/point6"
        android:layout_width="6dp"
        android:layout_height="6dp"
        android:src="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="@id/center"
        app:layout_constraintCircle="@id/center"
        app:layout_constraintCircleAngle="300"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintStart_toStartOf="@id/center"
        app:tint="@android:color/black" />


    <ImageView
        android:id="@+id/point7"
        android:layout_width="6dp"
        android:layout_height="6dp"
        android:src="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="@id/center"
        app:layout_constraintCircle="@id/center"
        app:layout_constraintCircleAngle="330"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintStart_toStartOf="@id/center"
        app:tint="@android:color/black" />

    <ImageView
        android:id="@+id/point8"
        android:layout_width="6dp"
        android:layout_height="6dp"
        android:src="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="@id/center"
        app:layout_constraintCircle="@id/center"
        app:layout_constraintCircleAngle="360"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintStart_toStartOf="@id/center"
        app:tint="@android:color/black" />

    <ImageView
        android:id="@+id/point9"
        android:layout_width="6dp"
        android:layout_height="6dp"
        android:src="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="@id/center"
        app:layout_constraintCircle="@id/center"
        app:layout_constraintCircleAngle="30"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintStart_toStartOf="@id/center"
        app:tint="@android:color/black" />

    <ImageView
        android:id="@+id/point10"
        android:layout_width="6dp"
        android:layout_height="6dp"
        android:src="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="@id/center"
        app:layout_constraintCircle="@id/center"
        app:layout_constraintCircleAngle="60"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintStart_toStartOf="@id/center"
        app:tint="@android:color/black" />

    <ImageView
        android:id="@+id/point11"
        android:layout_width="60dp"
        android:layout_height="6dp"
        android:src="@drawable/android_11"
        app:layout_constraintBottom_toBottomOf="@id/center"
        app:layout_constraintCircle="@id/center"
        app:layout_constraintCircleAngle="90"
        app:layout_constraintCircleRadius="110dp"
        app:layout_constraintStart_toStartOf="@id/center" />


</androidx.constraintlayout.motion.widget.MotionLayout>